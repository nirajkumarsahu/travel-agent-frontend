    "babel-eslint": "^10.0.2",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "hoist-non-react-statics": "^3.3.0",
    "intersection-observer": "^0.7.0",
    "invariant": "^2.2.4",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "mv": "^2.1.1",
