{
  "name": "sbt-framework",
  "version": "1.0.0",
  "description": "Basic next js app with react",
  "main": "pages/index.js",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "rimraf ./.next && next build",
    "bundle-analysis-server": "BUNDLE_ANALYZE=server npm run build",
    "bundle-analysis-client": "BUNDLE_ANALYZE=browser npm run build",
    "bundle-analysis-both": "BUNDLE_ANALYZE=both npm run build",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "eslint app --ext .js --fix",
